{
  "$id": "http://example.com/publishLearningExperience_OK_RESPONSE.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "required": [
    "_id",
    "previousState",
    "transitionState",
    "_created",
    "reasons"
  ],
  "properties": {
    "previousState": {
      "type": "string",
      "example": "COMPOSED"
    },
    "transitionState": {
      "type": "string",
      "example": "PUBLISHED"
    },
    "reasons":{
      "type": "string"
    },
    "_id": {
      "type": "string",
      "format": "uuid",
      "example": "32f42ce8-61dd-4bbc-bba5-1fdc03a0af9f"
    },
    "_created": {
      "type": "string",
      "format": "date-time"
        }
  }
}